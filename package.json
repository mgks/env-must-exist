{
  "name": "env-must-exist",
  "version": "0.1.0",
  "description": "A short, honest description of what this package does.",
  "type": "module",
  "bin": {
    "env-must-exist": "dist/cli.js"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "scripts": {
    "test": "node --import=tsx --test test/*.ts",
    "build": "tsup",
    "prepublishOnly": "npm run build && npm test",
    "lint": "xo"
  },
  "keywords": [
    "cli",
    "env",
    "environment",
    "assert",
    "check",
    "ci",
    "pipeline",
    "validate"
  ],
  "author": {
    "name": "mgks",
    "url": "https://mgks.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgks/env-must-exist.git"
  },
  "bugs": {
    "url": "https://github.com/mgks/env-must-exist/issues"
  },
  "homepage": "https://github.com/mgks/env-must-exist#readme",
  "funding": "https://github.com/sponsors/mgks",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "xo": "^0.56.0"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "unicorn/filename-case": "off"
    }
  }
}